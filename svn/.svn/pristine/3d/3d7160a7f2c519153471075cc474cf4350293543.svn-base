#include "bitstream.h"
#include <iostream>

using namespace std;

int main() {
    ibitstream in("in");
    obitstream out("out", ios::app);
    ifstream fin("in", ios::ate);
    int size = (int)fin.tellg();
    bool b;
    deque<bool> d{0, 1, 1, 0, 0, 1, 0, 0};
    out << d;
    for (int i = 0; i < size * 8; ++i) {
        in >> b;
        out << b;
    }
    return 0;
}